for item in plan_items:
    for letter in item:
        print(letter, end='', flush=True)
        time.sleep(0.02)
    print()

for letter in "\nÐ¡Ð¿Ð°ÑÐ¸Ð±Ð¾ Ð·Ð° Ð¸Ð³Ñ€Ñƒ! ÐŸÐ¾Ð¿Ñ€Ð¾Ð±ÑƒÐ¹Ñ‚Ðµ Ð´Ñ€ÑƒÐ³Ð¸Ðµ ÐºÐ¾Ð¼Ð±Ð¸Ð½Ð°Ñ†Ð¸Ð¸ Ð´Ð»Ñ Ñ€Ð°Ð·Ð½Ñ‹Ñ… ÐºÐ¾Ð½Ñ†Ð¾Ð²Ð¾Ðº.":
    print(letter, end='', flush=True)
    time.sleep(0.02)
print()
